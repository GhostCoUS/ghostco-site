{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/merch/*", "/sideline/*", "/support/*", "/labs/*"]
  },
  "routes": [
    {
      "route": "/merch/*",
      "serve": "/merch/index.html"
    },
    {
      "route": "/sideline/*",
      "serve": "/sideline/index.html"
    },
    {
      "route": "/support/*",
      "serve": "/support/index.html"
    },
    {
      "route": "/labs/*",
      "serve": "/labs/index.html"
    }
  ],
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html"
    }
  },
  "globalHeaders": {
    "Access-Control-Allow-Origin": "*"
  },
  "platform": {
    "routes": {
      "merch.ghostco.us": "/merch/index.html",
      "sideline.ghostco.us": "/sideline/index.html",
      "labs.ghostco.us": "/labs/index.html"
    }
  }
}
