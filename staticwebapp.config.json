{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/merch/*", "/sideline/*", "/support/*", "/labs/*"]
  },
  "routes": [
    {
      "route": "/merch/*",
      "serve": "/merch/index.html"
    },
    {
      "route": "/sideline/*",
      "serve": "/sideline/index.html"
    },
    {
      "route": "/support/*",
      "serve": "/support/index.html"
    },
    {
      "route": "/labs/*",
      "serve": "/labs/index.html"
    },
    {
      "route": "https://merch.ghostco.us/*",
      "rewrite": "/merch/index.html"
    },
    {
      "route": "https://sideline.ghostco.us/*",
      "rewrite": "/sideline/index.html"
    },
    {
      "route": "https://labs.ghostco.us/*",
      "rewrite": "/labs/index.html"
    }
  ],
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html"
    }
  }
}
