 "routes": [
    {
      "route": "/merch/*",
      "serve": "/merch/index.html"
    },
    {
      "route": "/sideline/*",
      "serve": "/sideline/index.html"
    },
    {
      "route": "/labs/*",
      "serve": "/labs/index.html"
    },
    {
      "route": "https://merch.ghostco.us/*",
      "serve": "/merch/index.html"
    },
    {
      "route": "https://sideline.ghostco.us/*",
      "serve": "/sideline/index.html"
    },
    {
      "route": "https://labs.ghostco.us/*",
      "serve": "/labs/index.html"
    }
  ],
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html"
    }
  }
}
