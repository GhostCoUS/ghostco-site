{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/merch/*", "/sideline/*", "/labs/*"]
  },
  "routes": [
    {
      "route": "/*",
      "serve": "/merch/index.html",
      "headers": {
        "Host": "merch.ghostco.us"
      }
    },
    {
      "route": "/*",
      "serve": "/sideline/index.html",
      "headers": {
        "Host": "sideline.ghostco.us"
      }
    },
    {
      "route": "/*",
      "serve": "/labs/index.html",
      "headers": {
        "Host": "labs.ghostco.us"
      }
    },
    {
      "route": "/merch/*",
      "serve": "/merch/index.html"
    },
    {
      "route": "/sideline/*",
      "serve": "/sideline/index.html"
    },
    {
      "route": "/labs/*",
      "serve": "/labs/index.html"
    }
  ],
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html"
    }
  }
}
